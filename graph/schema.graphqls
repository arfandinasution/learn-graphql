# GraphQL schema example
#
# https://gqlgen.com/getting-started/

type Todo {
  id: ID!
  text: String!
  done: Boolean!
  user: User!
}

type User{
  id: ID!
  email: String!
  password: String!
}

type Address {
  id: ID!
  jalan: String!
}

type Query {
  todos: [Todo!]!
  address: [Address!]!
  user: [User!]!
}

input NewTodo {
  text: String!
  userId: String!
}

type Mutation {
  createTodo(input: NewTodo!): Todo!
}